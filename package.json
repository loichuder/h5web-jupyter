  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/launcher": "^2.1.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.9.3"
  },
